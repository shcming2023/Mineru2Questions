  {
    "id": 29,
    "type": "text",
    "page_idx": 1,
    "text": "List of formulas"
  },
  {
    "id": 30,
    "type": "text",
    "page_idx": 1,
    "text": "Area, $A$ , of triangle, base $b$ , height $h$ ."
  },
  {
    "id": 31,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nA = \\frac {1}{2} b h\n$$"
  },
  {
    "id": 32,
    "type": "text",
    "page_idx": 1,
    "text": "Area, $A$ , of circle of radius $r$ ."
  },
  {
    "id": 33,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nA = \\pi r ^ {2}\n$$"
  },
  {
    "id": 34,
    "type": "text",
    "page_idx": 1,
    "text": "Circumference, $C$ , of circle of radius $r$ ."
  },
  {
    "id": 35,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nC = 2 \\pi r\n$$"
  },
  {
    "id": 36,
    "type": "text",
    "page_idx": 1,
    "text": "Curved surface area, $A$ , of cylinder of radius $r$ , height $h$ ."
  },
  {
    "id": 37,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nA = 2 \\pi r h\n$$"
  },
  {
    "id": 38,
    "type": "text",
    "page_idx": 1,
    "text": "Curved surface area, $A$ , of cone of radius $r$ , sloping edge $l$ ."
  },
  {
    "id": 39,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nA = \\pi r l\n$$"
  },
  {
    "id": 40,
    "type": "text",
    "page_idx": 1,
    "text": "Surface area, $A$ , of sphere of radius $r$ ."
  },
  {
    "id": 41,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nA = 4 \\pi r ^ {2}\n$$"
  },
  {
    "id": 42,
    "type": "text",
    "page_idx": 1,
    "text": "Volume, $V$ , of prism, cross-sectional area $A$ , length $l$ ."
  },
  {
    "id": 43,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nV = A l\n$$"
  },
  {
    "id": 44,
    "type": "text",
    "page_idx": 1,
    "text": "Volume, $V$ , of pyramid, base area $A$ , height $h$ ."
  },
  {
    "id": 45,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nV = \\frac {1}{3} A h\n$$"
  },
  {
    "id": 46,
    "type": "text",
    "page_idx": 1,
    "text": "Volume, $V$ , of cylinder of radius $r$ , height $h$ ."
  },
  {
    "id": 47,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nV = \\pi r ^ {2} h\n$$"
  },
  {
    "id": 48,
    "type": "text",
    "page_idx": 1,
    "text": "Volume, $V$ , of cone of radius $r$ , height $h$ ."
  },
  {
    "id": 49,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nV = \\frac {1}{3} \\pi r ^ {2} h\n$$"
  },
  {
    "id": 50,
    "type": "text",
    "page_idx": 1,
    "text": "Volume, $V$ , of sphere of radius $r$ ."
  },
  {
    "id": 51,
    "type": "equation",
    "page_idx": 1,
    "text": "$$\nV = \\frac {4}{3} \\pi r ^ {3}\n$$"
  },
